print("\t\t *")  

for a in range(1, 11, 1):  
    for b in range(10, a, -1):
        print(" ", end=" ")
    for c in range(1, a, 1):
        print("*", end=" ")
    for d in range(1, a, 1):
        print("*", end=" ")
    print()

for a in range(1, 11, 1):  
    for b in range(1, a, 1):
        print(" ", end=" ")
    for c in range(10, a, -1):
        print("*", end=" ")
    for d in range(10, a, -1):
        print("*", end=" ")
    print()

print("\t\t *") 


